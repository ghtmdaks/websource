<?xml version="1.0" encoding="UTF-8"?>
<Context>
	<Resource name="jdbc/myoracle" auth="Container"
		type="javax.sql.DataSource" driverClassName="oracle.jdbc.OracleDriver"
		url="jdbc:oracle:thin:@localhost:1521:xe" username="c##java"
		password="12345" maxTotal="20" maxIdle="10" maxWaitMillis="-1" />
</Context>
<!--url : 사용자가 쓰고있는 oracle 을 적어야하고 username = system 이름 password = 비밀번호 -->